server {
    server_name 51.250.16.243 taskitask.myftp.org;


    location /api/ {
	proxy_pass http://127.0.0.1:8000;
    }


    location /admin/ {
	proxy_pass http://127.0.0.1:8000;
    }


    location / {
	root   /var/www/taski;
	index  index.html index.htm;
	try_files $uri /index.html;
    }



    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/taskitask.myftp.org/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/taskitask.myftp.org/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = taskitask.myftp.org) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    

    listen 80;
    server_name 51.250.16.243 taskitask.myftp.org;
    return 404; # managed by Certbot


}
server {
    server_name 51.250.16.243 kittygram.line.pm;


    location /api/ {
	proxy_pass http://127.0.0.1:8080;
	client_max_body_size 20M;
    }


    location /media/ {
	alias /var/www/kittygram/media/;
    }


    location /admin/ {
	proxy_pass http://127.0.0.1:8080;
	client_max_body_size 20M;
    }


    location / {
	root  /var/www/kittygram;
	index index.html index.htm;
	try_files $uri /index.html;
    }


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/kittygram.line.pm/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/kittygram.line.pm/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}

server {
    if ($host = kittygram.line.pm) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    server_name 51.250.16.243 kittygram.line.pm;
    listen 80;
    return 404; # managed by Certbot


}
